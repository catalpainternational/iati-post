    "version": "0.2.0",
    "configurations": [
        {"name":"Python: Current File (Integrated Terminal)","type":"python","request":"launch","program":"${file}","console":"integratedTerminal"},

        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/iati_post/manage.py",
            "console": "integratedTerminal",
            "args": ["runserver", "--noreload", "--nothreading"],
            "django": true
        },
        {
            "name": "Python: Django Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/iati_post/manage.py",
            "console": "integratedTerminal",
            "args": ["test",],
            "django": true
        },
        {
            "name": "Python: Django Shell",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/iati_post/manage.py",
            "console": "integratedTerminal",
            "args": ["shell_plus"],
            "django": true
        },
        {
            "name": "Python: Django runworker",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/iati_post/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runworker",
                "iati",
                "organisation",
                "request",
                "request-process"
            ],
            "django": true
        }
    ]
